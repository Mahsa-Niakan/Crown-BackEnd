{
  "name": "Rate",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "userId": {
      "type": "number",
      "required": true
    },
    "comment": {
      "type": "string"
    },
    "likes": {
      "type": "number",
      "default": 0
    },
    "customer": {
      "type": "number",
      "required": true
    },
    "type": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "appUser": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "Admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "addNewRate"
    }
  ],
  "methods": {}
}
